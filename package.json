    "@iconify/vue": "^4.1.1",
    "@vee-validate/zod": "^4.11.3",
    "@vueuse/core": "^10.4.1",
    "convex": "^1.1.1",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "open-props": "^1.5.15",
    "vee-validate": "^4.11.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
